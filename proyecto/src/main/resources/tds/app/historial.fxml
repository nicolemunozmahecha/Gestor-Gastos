<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox spacing="10" xmlns="http://javafx.com/javafx/25" xmlns:fx="http://javafx.com/fxml/1" fx:controller="tds.vista.MostrarHistorialController">
   <padding>
      <Insets bottom="10" left="10" right="10" top="10" />
   </padding>
   <children>
      <TableView VBox.vgrow="ALWAYS">
         <columns>
            <TableColumn prefWidth="50.0" text="id" />
            <TableColumn prefWidth="200.0" text="Mensaje" />
            <TableColumn prefWidth="120.0" text="FechaHora" />
            <TableColumn prefWidth="100.0" text="Alerta" />
            <TableColumn prefWidth="60.0" text="Leida" />
         </columns>
         <columnResizePolicy>
            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
         </columnResizePolicy>
      </TableView>
      
      <HBox alignment="CENTER">
         <children>
            <Button fx:id="atras" mnemonicParsing="false" onAction="#atras" text="AtrÃ¡s" />
         </children>
      </HBox>
   </children>
</VBox>
